{
  "cli": {
    "version": ">= 15.0.13",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "bundleIdentifier": "eu.bruceroettgers.mapvoyage.development"
      },
      "android": {
        "packageName": "eu.bruceroettgers.mapvoyage.development"
      },
      "configure": {
        "appName": "Mapvoyage Development"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "bundleIdentifier": "eu.bruceroettgers.mapvoyage.preview"
      },
      "android": {
        "packageName": "eu.bruceroettgers.mapvoyage.preview"
      },
      "configure": {
        "appName": "Mapvoyage Preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "bundleIdentifier": "eu.bruceroettgers.mapvoyage"
      },
      "android": {
        "packageName": "eu.bruceroettgers.mapvoyage"
      },
      "configure": {
        "appName": "Mapvoyage"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
